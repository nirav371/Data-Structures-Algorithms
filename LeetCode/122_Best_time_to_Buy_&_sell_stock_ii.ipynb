    "def maxProfit(self, prices: List[int]) -> int:\n",
    "        max_profit = 0\n",
    "        for i in range(0, len(prices)-1):\n",
    "            if prices[i] < prices[i+1]:\n",
    "                max_profit += prices[i+1]-prices[i]\n",
    "        return max_profit"
   ]
  }
