    "def differenceOfSums(self, n: int, m: int) -> int:\n",
    "        list1, list2 = [], []\n",
    "        for i in range(1,n+1):\n",
    "            if i%m == 0:\n",
    "                list1.append(i)\n",
    "            else:\n",
    "                list2.append(i)\n",
    "        return sum(list2)-sum(list1)"
   ]
